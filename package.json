{
  "name": "@lyrafinance/lyra-js",
  "version": "0.0.26",
  "description": "A JavaScript SDK for the Lyra Protocol.",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "homepage": "www.lyra.finance",
  "author": "Lyra Finance",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyra-finance/lyra-js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "tsc": "yarn build",
    "watch": "concurrently \"tsc --project tsconfig.esm.json --watch\" \"tsc --project tsconfig.cjs.json --watch\"",
    "script": "env-cmd -f scripts/.env.local ts-node --project tsconfig.node.json scripts/index.ts",
    "typegen": "ts-node --project tsconfig.node.json src/contracts/typegen.ts",
    "sync": "ts-node --project tsconfig.node.json src/contracts/sync.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint '**/*.ts'",
    "clean": "rimraf node_modules dist",
    "ts-prune": "ts-prune -p tsconfig.esm.json -i 'src/contracts/.*/typechain/|src/index.ts'",
    "prepare": "yarn build",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.6.1",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.21",
    "@types/underscore": "^1.11.4",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "concurrently": "^7.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^10.0.1",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "ts-prune": "^0.10.3",
    "tslib": "^2.4.0",
    "typechain": "^8.1.1",
    "typescript": "^4.7.3",
    "underscore": "^1.13.6",
    "yargs": "^17.3.1"
  }
}
