{
  "name": "@lyrafinance/lyra-js",
  "version": "0.0.16",
  "description": "A JavaScript SDK for Optimistic Ethereum and the Lyra Protocol.",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "homepage": "www.lyra.finance",
  "author": "Lyra Finance",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyra-finance/lyra-js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "dev": "concurrently \"tsc --project tsconfig.esm.json --watch\" \"tsc --project tsconfig.cjs.json --watch\"",
    "sync": "ts-node --project tsconfig.node.json src/contracts/sync.ts",
    "example": "env-cmd -f src/examples/.env.local ts-node --project tsconfig.node.json src/examples/index.ts",
    "generate-types": "yarn typechain --target ethers-v5 --out-dir src/contracts/typechain src/contracts/abis/*.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint '**/*.ts'",
    "clean": "rimraf node_modules dist",
    "prepare": "yarn build",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@ethersproject/address": "^5.6.0",
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/contracts": "^5.6.0",
    "@ethersproject/providers": "^5.6.0",
    "@ethersproject/strings": "^5.6.0",
    "@ethersproject/units": "^5.6.0",
    "@ethersproject/wallet": "^5.6.0",
    "ethers": "^5.6.0",
    "firebase": "^9.9.4",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^9.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.21",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "concurrently": "^7.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^10.0.1",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "tslib": "^2.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.7.3",
    "yargs": "^17.3.1"
  }
}
